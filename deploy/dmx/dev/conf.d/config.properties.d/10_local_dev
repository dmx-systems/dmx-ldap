### DMX Configuration for Docker Environments ###

# LDAP
dmx.ldap.protocol = LDAP
dmx.ldap.server = ldap
dmx.ldap.port = 389
dmx.ldap.manager = cn=admin,dc=example,dc=org
dmx.ldap.password = test1234
dmx.ldap.user_base = ou=users,dc=example,dc=org
dmx.ldap.user_attribute = uid
dmx.ldap.user_filter = (&(objectClass=inetOrgPerson)(memberof=cn=app_users,dc=example,dc=org))
dmx.ldap.user_member_group = cn=app_users,dc=example,dc=org
dmx.ldap.logging = DEBUG
dmx.ldap.user_creation.enabled = false
dmx.ldap.group_base = dc=example,dc=org
